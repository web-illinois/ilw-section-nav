{
    "name": "@illinois-toolkit/ilw-section-nav",
    "description": "Illinois Toolkit: Secondary navigation for a section of a website.",
    "repository": "github:web-illinois/ilw-section-nav",
    "private": false,
    "license": "MIT",
    "version": "1.1.0",
    "type": "module",
    "files": [
        "src/**",
        "dist/**",
        "builder/**"
    ],
    "exports": {
        ".": {
            "import": "./src/ilw-section-nav.js",
            "require": "./dist/ilw-section-nav.cjs",
            "default": "./dist/ilw-section-nav.js"
        }
    },
    "main": "./dist/ilw-section-nav.js",
    "scripts": {
        "dev": "vite",
        "build": "vite build --config vite.build.config.js --emptyOutDir",
        "preview": "vite preview",
        "prepack": "npm run build"
    },
    "dependencies": {
        "lit": "3.1.3"
    },
    "devDependencies": {
        "vite": "^5.4.8",
        "prettier": "^3.3.3",
        "postcss-nested": "^6.2.0"
    }
}
